  <body class="login">
    <div>
      <a class="hiddenanchor" id="signup"></a>
      <a class="hiddenanchor" id="signin"></a>

      <div class="login_wrapper">
           <div id="register" class="animate">
          <section class="login_content">
            <form action="/user/signup" method="POST">
              <h1>Create Account</h1>
              <div>
                <input type="text" class="form-control" placeholder="Username" name="username" required="" />
              </div>
              <div>
                <input type="email" class="form-control" placeholder="Email" name="email" required="" />
              </div>
              <div>
                <input type="password" class="form-control" placeholder="Password" name="password" required="" />
              </div>
              <div>
                <input type="text" id="private_key" placeholder=" private key" oninput="publickeyconvertion()">
              </div>
              <div>
                 <input type="hidden" class="form-control" id="demo" placeholder="public key" name="publickey" required="" />
              </div>
              <div>
                  <input type="hidden" name="_csrf" value="{{csrfToken}}">
                <button  type="submit" class="btn btn-default " href="">Sign Up</button>
                 {{!-- <button type="submit" class="signupbtn btn btn-success">Sign Up</button> --}}

              </div>

              <div class="clearfix"></div>

              <div class="separator">
                <p class="change_link">Already a member ?
                  <a href="/user/signin" class="to_register"> Log in </a>
                </p>

                <div class="clearfix"></div>
                <br />

                <div>
                  <h1><i class="fa fa-paw"></i> Chainflux </h1>
                  <p>Â©2018 All Rights Reserved. Chainflux. Privacy and Terms</p>
                </div>
              </div>
            </form>
          </section>
        </div>
      </div>

    </div>
